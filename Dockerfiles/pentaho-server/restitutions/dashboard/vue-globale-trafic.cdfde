            "value" : "function postfetch(data) {\n    if (!data || !data.resultset || data.resultset.length === 0) {\n        return;\n    }\n\n    const row = data.resultset[0];\n    \n    const nbTrainsActifs = parseInt(row[0]); \n    const nbTrainsActifsHMoins1 = parseInt(row[1]);\n\n    const valElement = document.getElementById(\u0027nb_train_ciruclation_val\u0027);\n    if (valElement) {\n\n        valElement.textContent = nbTrainsActifs.toLocaleString(); \n    }\n\n    const progressionElement = document.getElementById(\u0027nb_train_ciruclation_progression\u0027);\n\n    if (progressionElement) {\n        let progression = 0;\n        let texteProgression = \u0022\u0022;\n        \n        progressionElement.classList.remove(\u0027text-success\u0027, \u0027text-danger\u0027);\n        \n        if (nbTrainsActifsHMoins1 \u003E 0) {\n            progression = ((nbTrainsActifs - nbTrainsActifsHMoins1) / nbTrainsActifsHMoins1) * 100;\n            \n\n            texteProgression = `${progression.toFixed(1)}%`;\n            progressionElement.textContent = (progression \u003E= 0 ? \u0027+\u0027 : \u0027\u0027) + texteProgression;\n\n            if (progression \u003E 0) {\n                progressionElement.classList.add(\u0027text-success\u0027); // Vert pour positif\n            } else if (progression \u003C 0) {\n                progressionElement.classList.add(\u0027text-danger\u0027); // Rouge pour négatif\n            }\n        } else if (nbTrainsActifs \u003E 0 \u0026\u0026 nbTrainsActifsHMoins1 === 0) {\n            progressionElement.textContent = \u0022Nouveau\u0022;\n            progressionElement.classList.add(\u0027text-success\u0027);\n        } else {\n            progressionElement.textContent = \u00220%\u0022;\n        }\n    }\n} "
