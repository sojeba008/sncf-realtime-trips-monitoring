            "value" : "function postfetchHeatmap(data) {\n\n    var delayHeatmapOptions = {\n    series: [], \n    chart: {\n        height: 400,\n        type: \u0027heatmap\u0027,\n        toolbar: {\n            show: true\n        }\n    },\n    dataLabels: {\n        enabled: false \n    },\n    colors: [\u0022#FF0000\u0022], \n    title: {\n        text: \u0027\u0027,\n        align: \u0027left\u0027\n    },\n    xaxis: {\n        type: \u0027category\u0027,\n        categories: [] \n    },\n    plotOptions: {\n        heatmap: {\n            shadeIntensity: 0.5,\n            radius: 0,\n            useFillColorAsStroke: true,\n            colorScale: {\n\n                ranges: [{\n                    from: 0,\n                    to: 0.5,\n                    name: \u0027Faible (\u003C 0.5 min)\u0027,\n                    color: \u0027#00A100\u0027 // Vert\n                },\n                {\n                    from: 0.5,\n                    to: 2,\n                    name: \u0027Modéré (0.5 - 2 min)\u0027,\n                    color: \u0027#FFB200\u0027 // Jaune/Orange\n                },\n                {\n                    from: 2,\n                    to: 100, // Une valeur haute\n                    name: \u0027Élevé (\u003E 2 min)\u0027,\n                    color: \u0027#FF0000\u0027 // Rouge\n                }\n                ]\n            }\n        }\n    }\n};\n\nvar delayHeatmapChart = new ApexCharts(document.querySelector(\u0022#delayHeatmapChart\u0022), delayHeatmapOptions);\ndelayHeatmapChart.render();\n    \n    if (typeof delayHeatmapChart === \u0027undefined\u0027) {\n        console.error(\u0022Le graphique Heatmap n\u0027est pas initialisé.\u0022);\n        return;\n    }\n\n    if (!data || !data.resultset || data.resultset.length === 0) {\n        console.warn(\u0022Aucune donnée reçue pour la Heatmap.\u0022);\n        return;\n    }\n\n    const regions = new Set();\n    const hours = new Set();\n    const dataMap = {}; \n\