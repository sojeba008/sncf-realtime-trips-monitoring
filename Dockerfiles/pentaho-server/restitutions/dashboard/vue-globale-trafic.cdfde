            "value" : "function postfetchTrains(data) {\n    \n    \n    \n    var evolutionNbTrainOptions = {\n    series: [{\n        name: \u0027Trains Actifs (Aujourd\\\u0027hui)\u0027,\n        data: []\n    }, {\n        name: \u0027Trains Actifs (Hier)\u0027,\n        data: []\n    }],\n    chart: {\n        height: 320,\n        type: \u0027line\u0027,\n        zoom: {\n            enabled: false\n        },\n        toolbar: {\n            show: true\n        }\n    },\n    dataLabels: {\n        enabled: false\n    },\n    stroke: {\n        curve: \u0027smooth\u0027,\n        width: 3\n    },\n    title: {\n        text: \u0027\u0027,\n        align: \u0027left\u0027\n    },\n    grid: {\n        row: {\n            colors: [\u0027#f3f3f3\u0027, \u0027transparent\u0027], \n            opacity: 0.5\n        },\n    },\n    xaxis: {\n        categories: [], // Sera rempli par postfetchTrains (les heures)\n        title: {\n            text: \u0027Heure\u0027\n        }\n    },\n    yaxis: {\n        title: {\n            text: \u0027Nombre de Trains Actifs\u0027\n        },\n        min: 0,\n        // Optionnel: Définir les étapes pour que les étiquettes soient des nombres entiers\n        labels: {\n            formatter: function (val) {\n                return Math.round(val);\n            }\n        }\n    },\n    tooltip: {\n        y: {\n            formatter: function (val) {\n                return val.toLocaleString() + \u0022 trains\u0022;\n            }\n        }\n    },\n    colors: [\u0027#008FFB\u0027, \u0027#FEB019\u0027] \n};\n\nvar evolutionNbTrainChart = new ApexCharts(document.querySelector(\u0022#evolutionNbTrain\u0022), evolutionNbTrainOptions);\nevolutionNbTrainChart.render();\n    \n    if (typeof evolutionNbTrainChart === \u0027undefined\u0027) {\n        console.error(\u0022Le graphique ApexCharts n\u0027est pas initialisé.\u0022);\n        return;\n    }\n\n    if (!data || !data.resultset || data.resultset.length === 0) {\n     