r: function (y) {\n                if (typeof y !== \u0022undefined\u0022) {\n                    return y.toFixed(2) + \u0022 min\u0022;\n                }\n                return y;\n            }\n        }, {\n            formatter: function (y) {\n                if (typeof y !== \u0022undefined\u0022) {\n                    return Math.round(y) + \u0022 trains\u0022;\n                }\n                return y;\n            }\n        }]\n    }\n};\n\nvar delayVolumeChart = new ApexCharts(document.querySelector(\u0022#delayVolumeChart\u0022), delayVolumeChartOptions);\ndelayVolumeChart.render();\n    \n    if (typeof delayVolumeChart === \u0027undefined\u0027) {\n        console.error(\u0022Le graphique n\u0027est pas init.\u0022);\n        return;\n    }\n\n    if (!data || !data.resultset || data.resultset.length === 0) {\n        console.warn(\u0022Aucune donnée reçue pour le graphique Retard/Nb trains.\u0022);\n        return;\n    }\n\n    const categories = [];     \n    const seriesDelay = [];    \n    const seriesVolume = [];  \n\n\n    data.resultset.forEach(row =\u003E {\n        \n        const heure = row[0]; // Format H:MM\n        const avgDelay = parseFloat(row[1]);\n        const nbTrains = parseInt(row[3]);\n        \n        if (!isNaN(avgDelay) \u0026\u0026 !isNaN(nbTrains)) {\n            categories.push(heure);\n            seriesDelay.push(avgDelay);\n            seriesVolume.push(nbTrains);\n        }\n    });\n    \n    delayVolumeChart.updateOptions({\n        series: [{\n            name: \u0027Retard Moyen (min)\u0027,\n            data: seriesDelay\n        }, {\n            name: \u0027Nombre de Trains\u0027,\n            data: seriesVolume\n        }],\n        xaxis: {\n            categories: categories\n        }\n    });\n    \n    \n} "
