            "value" : "function postfetchRetardsParTypeTrain(data) {\n    if (!data || !data.resultset || data.resultset.length === 0) {\n        return;\n    }\n\n    const container = document.getElementById(\u0027retards-par-type-train-container\u0027);\n    const template = document.getElementById(\u0027train-type-template\u0027);\n\n    if (!container || !template) {\n        return;\n    }\n\n    container.innerHTML = \u0027\u0027;\n\n    data.resultset.forEach(row =\u003E {\n        const typeTrain = row[0] || \u0027Non identifié\u0027;\n        const retardMoyen = parseFloat(row[1]);\n        const nbTrains = parseInt(row[2]);\n\n        const clone = document.importNode(template.content, true);\n        const item = clone.querySelector(\u0027.train-type-item\u0027);\n\n        const initials = typeTrain.split(/\\s+/).map(word =\u003E word.charAt(0)).join(\u0027\u0027).toUpperCase().substring(0, 2);\n        const initialsElement = item.querySelector(\u0027.train-type-initials\u0027);\n        if (initialsElement) {\n            initialsElement.textContent = initials;\n            \n            initialsElement.classList.remove(\u0027bg-label-primary\u0027);\n            if (retardMoyen \u003E 5) {\n                initialsElement.classList.add(\u0027bg-label-danger\u0027);\n            } else if (retardMoyen \u003E 2) {\n                initialsElement.classList.add(\u0027bg-label-warning\u0027);\n            } else {\n                initialsElement.classList.add(\u0027bg-label-success\u0027);\n            }\n        }\n\n        const delayElement = item.querySelector(\u0027.train-average-delay\u0027);\n        if (delayElement) {\n            delayElement.textContent = `${retardMoyen.toFixed(1)} min`;\n        }\n\n        const nameElement = item.querySelector(\u0027.train-type-name\u0027);\n        if (nameElement) {\n            nameElement.textContent = typeTrain;\n        }\n\n        const volumeElement = item.querySelector(\u0027.train-volume\u0027);\n        if (volumeElement) {\n            volumeElement.textContent = nbTrains.toLocaleString(\u0027fr-FR\u0027);\n        }\n\n        container.appendChild(item);\n    });\n} "
