            "value" : "function postfetchTotalMinutesRetard(data) {\n    console.log(\u0022DATA\u0022, data)\n    if (!data || !data.resultset || data.resultset.length \u003C 2) {\n        return;\n    }\n\n    const aujourdHuiData = data.resultset[0];\n    const hierData = data?.resultset[1];\n    const minutesAujourdHui = parseInt(aujourdHuiData[1]);\n    let variationVsHier = 0;\n    let minutesHier = 0;\n    if(hierData) {\n        minutesHier = parseInt(hierData[1]);\n    \n        if (minutesHier !== 0 \u0026\u0026 !isNaN(minutesAujourdHui) \u0026\u0026 !isNaN(minutesHier)) {\n            variationVsHier = ((minutesAujourdHui - minutesHier) / minutesHier) * 100;\n        } else if (minutesHier === 0 \u0026\u0026 minutesAujourdHui \u003E 0) {\n            variationVsHier = 1;\n        }\n    }\n    \n    const valElement = document.getElementById(\u0027nb_minutes_retard_ajd\u0027);\n    console.log(\u0022valElement\u0022, valElement);\n    if (valElement \u0026\u0026 !isNaN(minutesAujourdHui)) {\n        valElement.textContent = minutesAujourdHui.toLocaleString(\u0027fr-FR\u0027) + \u0027 min\u0027;\n    } else if (valElement) {\n        valElement.textContent = \u0027N/A\u0027;\n    }\n\n    const progressionElement = document.getElementById(\u0027nb_minutes_retard_progression_ajd\u0027);\n    const iconElement = progressionElement ? progressionElement.previousElementSibling : null;\n\n    if (progressionElement) {\n        progressionElement.classList.remove(\u0027text-success\u0027, \u0027text-danger\u0027);\n        if (iconElement) {\n            iconElement.className = \u0027\u0027;\n            iconElement.classList.add(\u0027icon-sm\u0027);\n        }\n        \n        if (!isNaN(variationVsHier) \u0026\u0026 isFinite(variationVsHier)) {\n            const texteProgression = `${Math.abs(variationVsHier).toFixed(1)}%`;\n            const signe = variationVsHier \u003E= 0 ? \u0027+\u0027 : \u0027\u0027;\n            \n            progressionElement.textContent = signe + texteProgression;\n\n            if (variationVsHier \u003E 0) {\n                progressionElement.classList.add(\u0027text-danger\u0027);\n                if (iconElement) iconElement.classList.add(\u0027ri\u0027, \u0027ri-arrow-up-s-line\u0027);\n            } else if (variationVsHier \u003C 0) {\n                progressionElement.classList.add(\u0027text-success\u0027);\n                if (iconElement) iconElement.classList.add(\u0027ri\u0027, \u0027ri-arrow-down-s-line\u0027);\n            } else {\n                progressionElement.textContent = \u00270.0%\u0027;\n                progressionElement.classList.add(\u0027text-muted\u0027);\n                if (iconElement) iconElement.classList.add(\u0027ri\u0027, \u0027ri-dash-line\u0027);\n            }\n        } else {\n            progressionElement.textContent = \u0022N/A\u0022;\n            progressionElement.classList.add(\u0027text-muted\u0027);\n            if (iconElement) iconElement.classList.add(\u0027ri\u0027, \u0027ri-dash-line\u0027);\n        }\n    }\n\n    const hierElement = document.getElementById(\u0027nb_minutes_retard_hier\u0027);\n    if (hierElement \u0026\u0026 !isNaN(minutesHier)) {\n        hierElement.textContent = `vs. ${minutesHier.toLocaleString(\u0027fr-FR\u0027)} min hier`;\n    } else if (hierElement) {\n        hierElement.textContent = \u0027vs. N/A hier\u0027;\n    }\n} "
